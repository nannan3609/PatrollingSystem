                    <!--<input class="checkItem" type="checkbox" value="背水面坝坡">背水面坝坡&nbsp;&nbsp;&nbsp;-->
                    <!--<input class="checkItem" type="checkbox" value="泄洪洞">泄洪洞&nbsp;&nbsp;&nbsp;-->
                    <!--<input class="checkItem" type="checkbox" value="坝顶">坝顶&nbsp;&nbsp;&nbsp;-->
